CREATE TABLE product (
    id SERIAL PRIMARY KEY,
    name VARCHAR(255) NOT NULL,
    quantity INTEGER,
    rent INTEGER,
    product_type VARCHAR(255),
    deleted BOOLEAN NOT NULL,
    date_created BIGINT NOT NULL,
    date_modified BIGINT
);

CREATE TABLE "user" (
    id SERIAL PRIMARY KEY,
    user_name VARCHAR(255) NOT NULL,
    role VARCHAR(255),
    deleted BOOLEAN NOT NULL,
    date_created BIGINT NOT NULL,
    date_modified BIGINT
);